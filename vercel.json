{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://backend-dun-iota.vercel.app/:path*"
      }
    ],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://your-frontend.vercel.app" },
          { "key": "Access-Control-Allow-Credentials", "value": "true" }
        ]
      }
    ]
  }
  