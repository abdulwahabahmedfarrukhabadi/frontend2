{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://backend123-five.vercel.app/:path*"
      }
    ],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://frontend2-blush.vercel.app/" },
          { "key": "Access-Control-Allow-Credentials", "value": "true" }
        ]
      }
    ]
  }
  